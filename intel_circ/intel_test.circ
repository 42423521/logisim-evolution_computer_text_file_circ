      mosi_o     : out std_logic                     ;  -- Serial data output to SD card.&#13;
      miso_i     : in  std_logic                       -- Serial data input from SD card.&#13;
      );&#13;
  end component;&#13;
  &#13;
  signal NOTreset_i       : std_logic;	-- Enable read&#13;
